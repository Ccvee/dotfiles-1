# shell before

# Sourced by .bashrc and .zshrc
# Run on both bash and zsh, in interactive shells
# Runs right after loader, runs every new shell/subshell

# Load env programs
source_if_exists "$CHRUBY_PREFIX/share/chruby/chruby.sh"
source_if_exists "$NVM_DIR/nvm.sh"
has_program "hub" && eval "$(hub alias -s)"

# Reset aliases for shell
source "$HOME/.dotfiles/shell/aliases"

# vim: syn=sh :
