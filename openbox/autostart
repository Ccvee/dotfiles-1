# These things are run when an Openbox X Session is started.

# Keymaps
xmodmap "$HOME/src/davidosomething-config/potatoW510/.Xmodmap" &

# Start Thinkpad OSD daemon
if [ -x /usr/bin/tpb ] &&  [ -w /dev/nvram ] && [ -r /dev/nvram ]; then
  /usr/bin/tpb -d &
fi

# Compositing
compton -b &

# Background Image
feh --bg-scale $HOME/Dropbox/Public/blue-lego.png &

# Launcher
# Can start on its own but I turned it off
kupfer &

# Dock
# Can start on its own but I turned it off
docky &

# Tint Panel
tint2 -c $HOME/.config/tint2/tint2rc &

# Thinkpad specific battery icon using tp_smapi
# Starts on its own
#tp-battery-icon &

# Volume applet
volti &
